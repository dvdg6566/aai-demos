{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ReadWriteAccess",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::democ90896ba-development",
                "arn:aws:s3:::democ90896ba-development/*"
            ]
        },
        {
            "Sid": "ListBuckets",
            "Effect": "Allow",
            "Action": [
                "s3:ListALlMyBuckets"
            ],
            "Resource": "*"
        }
    ]
}